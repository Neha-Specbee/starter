{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "FullContact Docs",
  "colors": {
    "primary": "#404aaa",
    "light": "#58bedf",
    "dark": "#58bedf"
  },
  "appearance": {
    "default": "light"
  },
  "background": {
    "decoration": "gradient"
  },
  "favicon": "/favicon.png",
  "fonts": {
    "heading": {
      "family": "Rubik"
    },
    "body": {
      "family": "Rubik"
    }
  },
  "navigation": {
    "versions": [
      {
        "version": "v3",
        "tabs": [
          {
            "tab": "Home",
            "pages": [
              "index"
            ]
          },
          {
            "tab": "Guides",
            "pages": [
              {
                "group": "Getting started",
                "pages": [
                  "/docs/getting-started",
                  "/docs/intro-to-fc"
                ]
              },
              {
                "group": "How-to Guides",
                "pages": [
                  {
                    "group": "Run a Test",
                    "pages": [
                      "/docs/run-a-match-test"
                    ]
                  },
                  "/docs/generate-an-api-key",
                  "/docs/webtag-installation-instructions"
                ]
              },
              {
                "group": "Integrations",
                "pages": [
                  "/docs/compatibility-matrix",
                  {
                    "group": "End-to-End Integration Guides",
                    "pages": [
                      "/docs/end-to-end-guides",
                      "/docs/google-tag-manager",
                      "/docs/google-analytics"
                    ]
                  }
                ]
              },
              {
                "group": "Customer Recognition",
                "pages": [
                  "/docs/customer-recognition-overview"
                ]
              }
            ]
          },
          {
            "tab": "Changelog",
            "pages": [
              "/changelog"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "FAQ",
          "href": "https://www.fullcontact.com/faq",
          "icon": "circle-question"
        },
        {
          "anchor": "Contact Us",
          "href": "https://www.fullcontact.com/contact/",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://www.fullcontact.com"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://support.fullcontact.com/portal"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Let's Talk",
      "href": "https://www.fullcontact.com/contact/"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "youtube": "https://www.youtube.com/user/FullContactAPI",
      "linkedin": "https://www.linkedin.com/company/fullcontact-inc-"
    }
  }
}